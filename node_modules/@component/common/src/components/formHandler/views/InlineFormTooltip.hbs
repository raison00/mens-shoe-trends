<div id="ex-tooltip-error" class="form-notification-tooltip-error">
  <div class="close-grey-tiny"></div>
  {{#if instructions}}
    <div class="instructions">{{{copy}}}</div>
    <ul class="instructions">
      {{#each items}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
  {{else}}
    <div>{{copy}}</div>
  {{/if}}
</div>
