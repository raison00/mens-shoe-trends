<div class="productThumbnailImage {{#if @root.meta.properties.responsiveLazyLoadEnabled}}{{#ifCond @index '>' 11 }}pending-lazy-load{{/ifCond}}{{/if}}" id="{{identifier.productId}}">
      {{#unless detail.flags.masterProduct}}
        {{#ifCond @root.meta.context.regionCode '===' 'US'}}
          {{#unless detail.flags.giftProduct}}
            {{> component-product-thumbnail/bcom/wishlist-icon}}
          {{/unless}}
        {{/ifCond}}
      {{/unless}}
    	<div id="img_{{identifier.productId}}"  class="thumbnailImageContainer {{#ifCond traits.colors.selectedColor.imagery.additionalImageSource.length '>' 1}}altImage{{/ifCond}}" data-altimage="{{#ifCond traits.colors.selectedColor.imagery.additionalImageSource.length '>' 1}}true{{/ifCond}}">
    	{{#if traits.colors.selectedColor.imagery.primaryImage.filePath}}
    		<picture aria-hidden="true">
	    		<source {{#if @root.meta.properties.responsiveLazyLoadEnabled}}{{#ifCond @index '>' 11 }}data-{{/ifCond}}{{/if}}srcset="{{bcomImageSrc traits.colors.selectedColor.imagery.primaryImage.filePath traits.colors.selectedColor.imagery.urlTemplate}}&hei=350&wid=280" class="thumbnailImage" alt="{{{detail.brand}}} - {{{detail.name}}}" media="(max-width: 599px)" />
	    		<img {{#if @root.meta.properties.responsiveLazyLoadEnabled}}{{#ifCond @index '>' 11 }}data-{{/ifCond}}{{/if}}src="{{bcomImageSrc traits.colors.selectedColor.imagery.primaryImage.filePath traits.colors.selectedColor.imagery.urlTemplate}}&hei=350&wid=280" class="thumbnailImage" alt="{{{detail.brand}}} - {{{detail.name}}}" />
	    	</picture>
    		{{#ifCond traits.colors.selectedColor.imagery.additionalImageSource.length '>' 1}}
	    		<picture aria-hidden="true">
	    			<source {{#if @root.meta.properties.responsiveLazyLoadEnabled}}{{#ifCond @index '>' 11 }}data-{{/ifCond}}{{/if}}srcset="{{bcomImageSrc traits.colors.selectedColor.imagery.additionalImageSource.[1].filePath traits.colors.selectedColor.imagery.urlTemplate}}&hei=350&wid=280" class="thumbnailImage alt" alt="{{{detail.brand}}} - {{{detail.name}}}" media="(max-width: 599px)" />
	    			<img {{#if @root.meta.properties.responsiveLazyLoadEnabled}}{{#ifCond @index '>' 11 }}data-{{/ifCond}}{{/if}}src="{{bcomImageSrc traits.colors.selectedColor.imagery.additionalImageSource.[1].filePath traits.colors.selectedColor.imagery.urlTemplate}}&hei=350&wid=280" class="thumbnailImage alt" alt="{{{detail.brand}}} - {{{detail.name}}}" />
	    		</picture>
    		{{else}}
    			<img {{#if @root.meta.properties.responsiveLazyLoadEnabled}}{{#ifCond @index '>' 11 }}data-{{/ifCond}}{{/if}}src="#" class="thumbnailImage alt hidden" alt="{{{detail.brand}}} - {{{detail.name}}}" />
    		{{/ifCond}}
    	{{else}}
  		 	<picture aria-hidden="true">
    		  <source id="img_{{identifier.productId}}" {{#if @root.meta.properties.responsiveLazyLoadEnabled}}{{#ifCond @index '>' 11 }}data-{{/ifCond}}{{/if}}srcset="{{bcomImageSrc imagery.primaryImage.filePath imagery.urlTemplate}}&hei=350&wid=280" class="thumbnailImage" alt="{{{detail.brand}}} - {{{detail.name}}}" media="(max-width: 599px)" />
    		  <img id="img_{{identifier.productId}}" {{#if @root.meta.properties.responsiveLazyLoadEnabled}}{{#ifCond @index '>' 11 }}data-{{/ifCond}}{{/if}}src="{{bcomImageSrc imagery.primaryImage.filePath imagery.urlTemplate}}&hei=350&wid=280" class="thumbnailImage" alt="{{{detail.brand}}} - {{{detail.name}}}" />
    		</picture>

  		{{#ifCond imagery.additionalImageSource.length '>' 1}}
		    <picture aria-hidden="true">
    		 	<source {{#if @root.meta.properties.responsiveLazyLoadEnabled}}{{#ifCond @index '>' 11 }}data-{{/ifCond}}{{/if}}srcset="{{bcomImageSrc imagery.additionalImageSource.[1].filePath imagery.urlTemplate}}&hei=350&wid=280" class="thumbnailImage alt" alt="{{{detail.brand}}} - {{{detail.name}}}" media="(max-width: 599px)" />
    		 	<img {{#if @root.meta.properties.responsiveLazyLoadEnabled}}{{#ifCond @index '>' 11 }}data-{{/ifCond}}{{/if}}src="{{bcomImageSrc imagery.additionalImageSource.[1].filePath imagery.urlTemplate}}&hei=350&wid=280" class="thumbnailImage alt" alt="{{{detail.brand}}} - {{{detail.name}}}" />
    		</picture>
  		{{else}}
  		 	<img {{#if @root.meta.properties.responsiveLazyLoadEnabled}}{{#ifCond @index '>' 11 }}data-{{/ifCond}}{{/if}}src="#" class="thumbnailImage alt hidden" alt="{{{detail.brand}}} - {{{detail.name}}}" />
  		{{/ifCond}}
  	{{/if}}
  	</div>
    <div class="jumboSwatch"><div id="jumboSwatch_{{identifier.productId}}"></div></div>
</div>
